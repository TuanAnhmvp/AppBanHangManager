<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appbanhang">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_tuananhshopfull_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_tuananhshopfull_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppBanHang"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.manage.appbanhang.activity.ContactActivity"
            android:exported="false" />
        <!--
             TODO: Before you run your application, you need a Google Maps API key.

             To get one, follow the directions here:

                https://developers.google.com/maps/documentation/android-sdk/get-api-key

             Once you have your API key (it starts with "AIza"), define a new property in your
             project's local.properties file (e.g. MAPS_API_KEY=Aiza...), and replace the
             "YOUR_API_KEY" string in this file with "${MAPS_API_KEY}".
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBqWCtW34TETwIA-1BjX0PVBVWEgudG_Ss" />

        <activity
            android:name="com.manage.appbanhang.activity.InformationActivity"
            android:exported="false"
            android:label="@string/title_activity_information" />
        <activity
            android:name="com.manage.appbanhang.activity.AddProductActivity"
            android:exported="false" />
        <activity
            android:name="com.manage.appbanhang.activity.ManageActivity"
            android:exported="true" />
        <activity
            android:name="com.manage.appbanhang.activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.manage.appbanhang.activity.SearchActivity"
            android:exported="false" />
        <activity
            android:name="com.manage.appbanhang.activity.ViewOrderActivity"
            android:exported="false" />
        <activity
            android:name="com.manage.appbanhang.activity.OrderActivity"
            android:exported="false" />
        <activity
            android:name="com.manage.appbanhang.activity.ResetPassActivity"
            android:exported="false" />
        <activity
            android:name="com.manage.appbanhang.activity.LoginActivity"
            android:exported="true" />
        <activity
            android:name="com.manage.appbanhang.activity.SignUpActivity"
            android:exported="true" />
        <activity
            android:name="com.manage.appbanhang.activity.CartActivity"
            android:exported="false" />
        <activity
            android:name="com.manage.appbanhang.activity.DetailActivity"
            android:exported="false" />
        <activity
            android:name="com.manage.appbanhang.activity.LaptopActivity"
            android:exported="false" />
        <activity
            android:name="com.manage.appbanhang.activity.PhoneActivity"
            android:exported="false" />
        <activity
            android:name="com.manage.appbanhang.activity.MainActivity"
            android:exported="true" />
        <service android:name="com.manage.appbanhang.Service.FirebaseMessageReceiver"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>